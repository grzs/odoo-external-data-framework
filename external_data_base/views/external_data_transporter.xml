<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Window actions -->
    <record id="external_data_transporter_action_window" model="ir.actions.act_window">
	<field name="name">External Data Transporter</field>
	<field name="res_model">external.data.transporter</field>
    </record>

    <!-- Views -->
    <record id="external_data_transporter_list_view" model="ir.ui.view">
	<field name="name">External Data transporters</field>
	<field name="model">external.data.transporter</field>
	<field name="arch" type="xml">
	    <tree>
		<field name="name"/>
		<field name="protocol"/>
	    </tree>
	</field>
    </record>
    
    <record id="external_data_transporter_form_view" model="ir.ui.view">
	<field name="name">External Data Transporter</field>
	<field name="model">external.data.transporter</field>
	<field name="arch" type="xml">
	    <form>
		<sheet>
		    <group>
			<field name="name"/>
			<field name="protocol"/>
		    </group>
		</sheet>
	    </form>
	</field>
    </record>
    
</odoo>
